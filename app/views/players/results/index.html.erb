<div class="container">
  <div class="row">
    <div class="col">
      <h1>Results</h1>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <ol>
        <% @games.each do |game| %>
          <li><%= game.player.email %> - <%= game.score %> points</li>
        <% end %>
      </ol>
    </div>
  </div>
</div>
